{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAllActionsForRoot",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::841162705069:root"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::my-frontend-bucket",
        "arn:aws:s3:::my-frontend-bucket/*"
      ]
    }
  ]
}
